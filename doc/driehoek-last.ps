/cm { 28.346457 mul } bind def
/mm { 2.8346457 mul } bind def

% A4 papier
/width {596} bind def
/height {842} bind def

% Draw a 1x1 cm grid
/grid {
    gsave
    .1 setlinewidth
    .7 1 1 setrgbcolor
    0 0 moveto
    0 1 13 {
        0 7 cm rlineto
        1 cm -7 cm rmoveto
    } for
    0 0 moveto
    0 1 7 {
        13 cm 0 rlineto
        -13 cm 1 cm rmoveto
    } for
    stroke
    grestore
} bind def

% Put a string from the symbol font, advance the point
% Usage: (text) pointsize symbol
/symbol {
    currentpoint moveto
    gsave
    /Symbol findfont
    exch scalefont
    setfont
    dup stringwidth pop exch
    show
    grestore
    0 rmoveto
} bind def


